<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgmenu" >

    <ImageButton
        android:id="@+id/ImageButton03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/ImageButton02"
        android:layout_toRightOf="@+id/ImageButton02"
        android:background="#000000"
        android:onClick="tbtn"
        android:src="@drawable/ticketstatus" />

    <ImageButton
        android:id="@+id/ImageButton01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/ImageButton03"
        android:layout_alignTop="@+id/imageButton1"
        android:background="#000000"
        android:onClick="Gabtn"
        android:src="@drawable/getassign" />

    <ImageButton
        android:id="@+id/ImageButton02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButton1"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="61dp"
        android:background="#000000"
        android:onClick="Abtn"
        android:src="@drawable/assign" />

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ImageButton02"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="61dp"
        android:background="#000000"
        android:onClick="Ibtn"
        android:src="@drawable/getissue" />

    <ListView
        android:id="@+id/listView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ImageButton03"
        android:layout_centerHorizontal="true" >
    </ListView>

</RelativeLayout>
